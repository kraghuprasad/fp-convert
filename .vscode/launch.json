{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: fp-convert (Blooper Specs)",
      "type": "debugpy",
      "request": "launch",
      "python": "${command:python.interpreterPath}", // Adjust this path
      "program": "fp_convert",
      "args": [
        "-k",
        "-d",
        "-c",
        "fp-convert.yml",
        "-f",
        "nunito",
        // "nunito:tabular:scaled=0.9",
        // "nunito:tabular:scaled=1.1",
        // "roboto:sfdefault:scaled=1.1",
        // "FiraSans:sfdefault:book:scaled=1.0",
        // "josefin:sfdefault:light:medium:scaled=1.1",
        // "josefin:sfdefault:medium:scaled=1.1",
        // "montserrat:defaultfam:tabular:lining:alternates",
        "docs/examples/blooper-specs/Blooper_Specifications.mm",
        "docs/examples/blooper-specs/Blooper_Specifications.pdf"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Python: fp-convert (CDC Planner)",
      "type": "debugpy",
      "request": "launch",
      "python": "${command:python.interpreterPath}", // Adjust this path
      "program": "fp_convert",
      "args": [
        "-k",
        "-d",
        "-c",
        "fp-convert.yml",
        "-f",
        // "nunito",
        // "nunito:tabular:scaled=0.9",
        // "nunito:tabular:scaled=1.1",
        // "roboto:sfdefault:scaled=1.1",
        // "FiraSans:sfdefault:book:scaled=1.0",
        // "josefin:sfdefault:light:medium:scaled=1.1",
        // "josefin:sfdefault:medium:scaled=1.1",
        "montserrat:defaultfam:tabular:lining:alternates",
        "docs/examples/cdc-planner/CDC_Planner.mm",
        "docs/examples/cdc-planner/CDC_Planner.pdf",
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
